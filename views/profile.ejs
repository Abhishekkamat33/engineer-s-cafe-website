<script> 
const key = "c9i1EFFsXejxJV9w2dlrgI2y6NJ7mv-4NFAqce2FXI0";
let currentIndex = 0; // Track the index of the current image

async function displayImage() {
    const response = await fetch("https://api.unsplash.com/search/photos?page=1&query=food", {
        headers: {
            Authorization: `Client-ID ${key}`
        }
    });
    const data = await response.json(); // Await the JSON parsing
    
    if (data.results && data.results.length > 0) {
        const images = data.results.map(result => result.urls.small); // Array of image URLs
        
        // Get the next image URL
        const imageUrl = images[currentIndex];
        
        const imagetag = document.querySelector(".displayimg");
        const image = document.createElement("img");
        image.src = imageUrl;
        imagetag.innerHTML = ""; // Clear previous image
        imagetag.appendChild(image);
        
        // Increment currentIndex for the next image (with wrapping)
        currentIndex = (currentIndex + 1) % images.length;
    } else {
        console.error("No images found in the API response.");
    }
}

// Set interval to call displayImage() every 5 seconds
setInterval(displayImage, 5000);

// Initial call to displayImage()
displayImage();


    window.addEventListener('scroll', function() {
    var aboutImg = document.querySelector('.about-img');
    var triggerPosition = aboutImg.getBoundingClientRect().top + window.scrollY;
    var triggerOffset = 100; // Adjust this value based on when you want the animation to trigger

    if (window.scrollY > triggerPosition - triggerOffset) {
      aboutImg.style.opacity = '1';
      aboutImg.style.transform = 'translateX(0)';
    }
  });</script>
